#Region  Activity Attributes 
	#FullScreen: False
	#IncludeTitle: False
#End Region
Sub Process_Globals
Dim t As Timer
End Sub

Sub Globals
	Dim webView1 As WebView
	Dim wvx As WebViewSettings
	Dim bn As AdView
	Dim it As InterstitialAd
End Sub

Sub Activity_Create(FirstTime As Boolean)
	webView1.Initialize("webView1")
	Activity.AddView(webView1,0,0,100%x,100%y)
	webView1.LoadUrl("http://4fbid.blogspot.com")
	wvx.setDisplayZoomControls(webView1,False)
	
	bn.Initialize2("bn","ca-app-pub-4173348573252986/8410444554",bn.SIZE_SMART_BANNER)
	Dim height As Int
	If GetDeviceLayoutValues.ApproximateScreenSize < 6 Then
		'phones
		If 100%x > 100%y Then height = 32dip Else height = 50dip
	Else
		'tablets
		height = 90dip
	End If
	Activity.AddView(bn, 0dip, 100%y - height, 100%x, height)
	bn.LoadAd
	
	it.Initialize("it","ca-app-pub-4173348573252986/2363910950")
	it.LoadAd
	
	t.Initialize("t",100)
	t.Enabled = False
	Activity.AddMenuItem("Refresh","rf")
	Activity.AddMenuItem("Exit App","ex")
End Sub

Sub rf_Click
	webView1.LoadUrl(webView1.Url)
End Sub

Sub ex_Click
	Activity.Finish
End Sub

Sub webView1_OverrideUrl(Url As String) As Boolean
	Log(Url)
	t.Enabled = True
End Sub

Sub Activity_KeyPress (KeyCode As Int) As Boolean 
	If KeyCode = KeyCodes.KEYCODE_BACK Then 
			webView1.Back
			Return True
		t.Enabled = True
	End If
End Sub

Sub t_Tick
	If it.Ready Then it.Show Else it.LoadAd
	t.Enabled = False
End Sub

Sub it_AdClosed
	it.LoadAd
End Sub

Sub Activity_Resume

End Sub